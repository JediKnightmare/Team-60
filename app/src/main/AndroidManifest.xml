<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.coditsuisse.team60.expensetracker" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddExpenseActivity"
            android:label="@string/title_activity_add_expense"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.coditsuisse.team60.expensetracker.MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity2"
            android:label="@string/title_activity_main2" >
        </activity>
        <activity
            android:name=".AddIncomeActivity"
            android:label="@string/title_activity_add_income"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.coditsuisse.team60.expensetracker.MainActivity" />
        </activity>
        <activity
            android:name=".ListTransaction"
            android:label="@string/title_activity_list_transaction"
            android:parentActivityName=".MainActivity2" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.coditsuisse.team60.expensetracker.MainActivity2" />
        </activity>
    </application>

</manifest>
